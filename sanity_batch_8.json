        "groupName": "Ingredients",
        "items": [
          {
            "_type": "ingredient",
            "_key": "ing_0",
            "amount": "3 1/2",
            "unit": "cup",
            "ingredient": "s unbleached all-purpose flour"
          },
          {
            "_type": "ingredient",
            "_key": "ing_1",
            "amount": "1",
            "unit": "tsp",
            "ingredient": "baking powder"
          },
          {
            "_type": "ingredient",
            "_key": "ing_2",
